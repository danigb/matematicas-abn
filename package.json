{
  "name": "abn",
  "version": "3.4.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run css:prod && react-scripts build",
    "test": "react-scripts test",
    "test:types": "tsc --noUnusedLocals",
    "lint": "tslint --fix 'src/**/*.{ts,tsx}'",
    "build:analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "css": "npm run css:dev",
    "css:watch": "postcss --watch src/styles/tailwind.css -o src/styles/index.css",
    "css:dev": "postcss src/styles/tailwind.css -o src/styles/index.css",
    "css:prod": "postcss src/styles/tailwind.css -o src/styles/index.css --env production"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@reach/router": "^1.3.3",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.9.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^12.12.24",
    "@types/reach__router": "^1.3.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "classcat": "^4.0.2",
    "debug": "^4.1.1",
    "lodash.debounce": "^4.0.8",
    "react": "^16.12.0",
    "react-css-collapse": "^4.0.4",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-hooks-global-state": "^1.0.0",
    "react-responsive": "^8.0.1",
    "react-router-dom": "^5.1.0",
    "react-scripts": "^3.3.0",
    "react-timer-hook": "^2.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "^5.0.0",
    "standardized-audio-context": "^23.1.2"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": false,
    "semi": true,
    "newline-before-return": true,
    "no-duplicate-variable": [
      true,
      "check-parameters"
    ],
    "no-var-keyword": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:types"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "@types/react-responsive": "^8.0.2",
    "serverless": "^1.61.1",
    "source-map-explorer": "^2.2.2",
    "nano": "^8.2.2",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-purgecss": "^2.0.3",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.1.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.7.4"
  }
}
